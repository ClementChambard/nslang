#include io.nsh

fn vfprintf(f: i64, s: i8*, ...*) {
  print("%i %s %i", f, s, vaarg<i64>);
}

fn vprintf(s: i8*, ...*) {
  return vfprintf(0, s, vaargs);
}

fn fprintf(f: i64, s: i8*, ...) {
  return vfprintf(f, s, vaargs);
}

fn printf(s: i8*, ...) {
  return vprintf(s, vaargs);
}

fn main() {
  printf("test", 1, 2, 3);
}
